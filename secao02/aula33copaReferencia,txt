//Tipos primitivos sao copiados enquando ARRAYS,OBETOS E FUNÇÕES  sao passados por referencia

let a = 1
let b = a

b += 1

console.log("a =",a," b =",b)

let c = [1 ,2 ,3]
let d = c

c.pop()
console.log("arreyC =",c,"arreyD =",d)

//como copiar as referencias
let e = [...c]
e.push(4)
console.log("arreyC =",c," arrayE =",e)